<Window x:Class="GTEventGenerator.GameGeneratorWindow"
	    xmlns = "http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
        xmlns:x = "http://schemas.microsoft.com/winfx/2006/xaml" 
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
	    Title="Gran Turismo 5/6 Event Editor" Width="960" Height="621"
        ResizeMode="CanMinimize"
        Loaded="GameGenerator_Load"
        Closing="Window_Closing">
    <DockPanel Background="#262626">
        <DockPanel DockPanel.Dock="Top">
            <Menu Name="menuStrip1" Height="22" HorizontalAlignment="Stretch" >
                <MenuItem Name="toolStripMenuItem1" Header="File">
                    <MenuItem Header="New Folder" Name="newEventToolStripMenuItem" Click="newEventToolStripMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>
                    <MenuItem Header="Import Folder (GT6)..." Name="importEventToolStripMenuItem" Click="importEventToolStripMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>
                    <MenuItem Header="Export Folder (GT6)..." Name="exportEventToolStripMenuItem" Click="exportEventToolStripMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>
                    <Separator />
                    <MenuItem Header="Import Event List (GT5)..." Click="importEventListToolStripMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>
                    <Separator />
                    <MenuItem Header="Exit" Click="exit_Click" Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>
                </MenuItem>
                <MenuItem Header="Edit">
                    <MenuItem Header="Minify XML's while Exporting" Name="minimizeXMLToolStripMenuItem" IsCheckable="True" Checked="minimizeXMLToolStripMenuItem_Checked" Unchecked="minimizeXMLToolStripMenuItem_Checked" Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>
                    <Separator />
                    <MenuItem Header="Regenerate ALL AI's..." Template="{DynamicResource SingleDropDownMenuItem}">
                        <MenuItem Header="Region/Names" Name="randomizeAINamesToolStripMenuItem" Click="randomizeAINamesToolStripMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <MenuItem Header="Tire Compounds" Name="randomizeAITireCompoundToolStripMenuItem" Click="randomizeAITireCompoundToolStripMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <Separator/>
                        <MenuItem Header="Roughness" Name="randomizeAIRoughnessToolStripMenuItem" Click="randomizeAIRoughnessMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <MenuItem Header="Base Skill" Name="randomizeAIBaseSkillToolStripMenuItem" Click="randomizeAIBaseSkillMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <MenuItem Header="Corner Skill"  Name="randomizeAICornerSkillToolStripMenuItem" Click="randomizeAICornerSkillMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <MenuItem Header="Braking Skill"  Name="randomizeAIBrakingSkillToolStripMenuItem" Click="randomizeAIBrakingSkillMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <MenuItem Header="Accel Skill"  Name="randomizeAIAccelSkillToolStripMenuItem" Click="randomizeAIAccelSkillMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <MenuItem Header="Starting Skill"  Name="randomizeAIStartSkillToolStripMenuItem" Click="randomizeAIStartSkillMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                        <MenuItem Header="(All Skills)"  Name="randomizeAISkillsToolStripMenuItem" Click="randomizeAISkillsMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}" IsEnabled="False"></MenuItem>
                    </MenuItem>

                </MenuItem>
                <MenuItem Header="Tools">
                    <MenuItem Header="Decrypt TED.." Name="decryptTedMenuItem" Click="decryptTedMenuItem_Click"  Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>
                    <MenuItem Header="Encrypt TED.." Name="encryptTedMenuItem" Click="encryptTedMenuItem_Click" Template="{DynamicResource SingleDropDownMenuItem}"></MenuItem>

                </MenuItem>
                <MenuItem Header="Help">
                    <MenuItem Header="About" Template="{DynamicResource SingleDropDownMenuItem}" Click="MenuItem_Click"></MenuItem>
                    <Separator />
                    <MenuItem Header="Enable Discord Rich Presence" Name="DiscordRichPresenceMenuItem" Template="{DynamicResource SingleDropDownMenuItem}" IsCheckable="True" Checked="DiscordRichPresenceMenuItem_Click" Unchecked="DiscordRichPresenceMenuItem_Click"></MenuItem>
                </MenuItem>
            </Menu>
            <ComboBox Name="cb_QuickEventPicker" SelectedIndex="0" DockPanel.Dock="Right" Width="340" Height="20" Margin="0,4,4,4" SelectionChanged="cb_QuickEventPicker_SelectionChanged"/>

            <Label DockPanel.Dock="Right" Width="100" Content="Current Event:" HorizontalAlignment="Right" HorizontalContentAlignment="Right" VerticalAlignment="Bottom"/>
        </DockPanel>
        <TabControl DockPanel.Dock="Bottom" Name="tabEvent" TabStripPlacement= "Top" IsTabStop="False" Margin="0,-2,0,0">
            <TabItem Name="tabFolderInfo">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal" >
                        <Image Width="16" Source="Resources/UI/TabLogos/FolderInfo.png"/>
                        <TextBlock Text="Folder Info" />
                    </StackPanel>
                </TabItem.Header>
                <Canvas Name="cantabFolderInfo" >
                    <GroupBox HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="391" Width="221" Height="131" >
                        <GroupBox.Header>
                            <TextBlock Text="Championship Options"/>
                        </GroupBox.Header>
                        <Canvas>
                            <Button Name="btnChampionshipRewards" Height="31" Width="190" TabIndex="6" IsEnabled="False" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="49" Click="btnChampionshipRewards_Click">Set Championship Rewards</Button>
                            <CheckBox Name="chkIsChampionship" Height="34" Width="190" TabIndex="5" HorizontalAlignment="Right" VerticalAlignment="Bottom" Unchecked="chkIsChampionship_CheckedChanged" Checked="chkIsChampionship_CheckedChanged" Canvas.Left="10" Canvas.Top="10" >Championship</CheckBox>
                        </Canvas>
                    </GroupBox>
                    <Label Name="label6" Content="Stars Needed" Height="24" Width="91"  TabIndex="8" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="182">
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">Stars Needed</TextBlock><LineBreak/>
                                The amount of stars needed in the current category (i.e Novice) for the player to access this folder. <LineBreak/>
                                Leave as 0 if you want it to be always unlocked.
                            </TextBlock>
                        </Label.ToolTip>
                    </Label>
                    <xctk:IntegerUpDown Name="iud_StarsNeeded" ValueChanged="iud_StarsNeeded_ValueChanged" Value="0" Height="20" Width="232" Canvas.Left="121" Canvas.Top="185" Minimum="0" Maximum="1000" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                    <Label Name="label_folderID" Content="Folder ID" Height="24" Width="91" TabIndex="8" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="211">
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">Folder ID</TextBlock><LineBreak/>
                                This is your *unique* folder ID, which will also point to the XML file number.<LineBreak/>
                                Make sure it also matches in MenuDB!
                                <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">play_style->play_type</TextBlock>
                            </TextBlock>
                        </Label.ToolTip>
                    </Label>
                    <xctk:IntegerUpDown Name="iud_FolderID" ValueChanged="iud_FolderID_ValueChanged" Height="20" Width="232" Canvas.Left="121" Canvas.Top="213" Value="1000" Minimum="0" Maximum="10000" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                    <Label Name="label_folderFileName" Content="Intl. Folder Name" Height="24" Width="108" TabIndex="8" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="240">
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">Internal Folder Name</TextBlock><LineBreak/>
                                Internal name of your folder file (GT6).<LineBreak/>
                                Example: sundaycup -> sundaycup.xml. <LineBreak/>
                                Make sure it also matches in MenuDB!
                            </TextBlock>
                        </Label.ToolTip>
                    </Label>
                    <TextBox Name="tb_FolderFileName" Canvas.Left="121" Canvas.Top="244" Width="232" TextChanged="tb_FolderFileName_TextChanged" />
                    <TextBox Name="txtGameParamName" Height="20" Width="232" TabIndex="1" TextChanged="txtFolderName_TextChanged" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" Canvas.Left="121" Canvas.Top="35" />
                    <TextBox Name="txtGameParamDesc" Height="92" Width="232" TabIndex="2" TextChanged="txtFolderDesc_TextChanged" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" Canvas.Left="121" Canvas.Top="60" />
                    <Label Name="label5" Height="24" Content="Folder Description" Width="108"  TabIndex="5" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="8" Canvas.Top="60" >
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">Folder Description</TextBlock><LineBreak/>
                                Description for your folder, which will display when hovering the folder.
                            </TextBlock>
                        </Label.ToolTip>
                    </Label>
                    <Label Name="label4" Height="24" Width="108"  TabIndex="4" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="8" Canvas.Top="33" >Folder Name</Label>
                    <Label Name="label3" Content="Folder Category" Height="27" Width="108" TabIndex="3" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="154" >
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">Folder Category</TextBlock><LineBreak/>
                                The location of the folder.<LineBreak/>
                                Make sure it also matches in MenuDB!
                            </TextBlock>
                        </Label.ToolTip>
                    </Label>
                    <ComboBox Name="cboEventCategory" Height="23" Width="232" TabIndex="3" HorizontalAlignment="Right" VerticalAlignment="Bottom" SelectionChanged="cboEventCategory_SelectedIndexChanged" Canvas.Left="123" Canvas.Top="157" />
                    <!--<Button Name="btnEventGenerate" Height="43" Width="137" TabIndex="8" Click="btnEventGenerate_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="659" Canvas.Top="479" >Generate Event</Button>
                    <Button Name="btnOpenMenuDB" Height="43" Width="137" TabIndex="7" Click="btnOpenMenuDB_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="801" Canvas.Top="479" >Open MenuDB</Button>-->
                </Canvas>
            </TabItem>
            <TabItem Name="tabEvents">
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal" >
                        <Image Width="16" Source="Resources/UI/TabLogos/Events.png"/>
                        <TextBlock Text="Events" />
                    </StackPanel>
                </TabItem.Header>
                <Canvas >
                    <GroupBox HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="512" Canvas.Left="325" Width="313" Canvas.Top="10" >
                        <GroupBox.Header>
                            <TextBlock Text="Event Information"/>
                        </GroupBox.Header>
                        <Canvas Margin="0,0,0,0" >
                            <Label Name="label_gameMode" Content="Game Mode" Height="23" Width="76" TabIndex="14" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="40">
                                <Label.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">Game Mode</TextBlock>
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">GT Mode Race</TextBlock>: Normal Race within the Novice to Extreme categories.
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">License</TextBlock>: License Test.
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Mission</TextBlock>: Moon/Senna Tribute for GT6. Allows car rewards if checks are bypassed. 
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Special Event (School)</TextBlock>: Used for Special Events (GT5).
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Rally Event</TextBlock>: Used for Special Rally Events (GT5).
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Arcade Race</TextBlock>: Regular Arcade Race.
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Seasonal Race </TextBlock>: Used for Seasonal Races (GT6/5?).
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Seasonal Time Trial</TextBlock>: Used for Seasonal Time Trials (GT6/5?) where the player tries to beat a lap time for a reward.
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Seasonal Drift Attack</TextBlock>: Used for Seasonal Drift Attacks (GT6/5?) where the player tries to beat a drift score within a track section.
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Overtake Mission (GT6)</TextBlock>: Used for Sierra Events - Also known as "Arcade Style Race"
                                        <LineBreak/>
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">game_mode</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <ComboBox Name="cb_gameModes" Height="23" Width="200" IsEnabled="False" TabIndex="13" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="91" Canvas.Top="41" SelectionChanged="cb_gameModes_SelectionChanged" />

                            <CheckBox Name="checkBox_SeasonalEvent" Content="Seasonal Event" Height="24" Width="281" TabIndex="56" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsEnabled="False" IsChecked="{Binding IsSeasonalEvent}" Canvas.Top="289" Canvas.Left="10">
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">Seasonal Event</TextBlock>
                                        <LineBreak/>
                                        Whether if the event is a Seasonal Event, if so, the event will have date requirements to enter, fill in the Start/End Dates (and additionally Ranking tab).
                                        <LineBreak/>
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">is_seasonal_event</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>

                            <Label Content="A/B Spec" Canvas.Left="10" Canvas.Top="68" Width="76">
                                <Label.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">A/B Spec</TextBlock>
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">A-Spec Only</TextBlock>: Player can participate in the race and drive the car.
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">B-Spec Only</TextBlock>: An AI will drive the car, and the player influences how the AI drives.
                                        <LineBreak/>
                                        - <TextBlock FontWeight="Bold">Both A and B</TextBlock>: Both are allowed.
                                        <LineBreak/>
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">play_style->bspec_type</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <ComboBox Name="cb_Spec" Height="23" Width="200" IsEnabled="False" TabIndex="13" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="91" Canvas.Top="71" SelectionChanged="cb_Spec_SelectionChanged" />

                            <Label Content="Play Type" Canvas.Left="10" Canvas.Top="99" Width="76">
                                <Label.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">Play Type</TextBlock>
                                        <LineBreak/>
                                        Whether if the event is a regular event or demo, should leave as Race
                                        - <TextBlock FontWeight="Bold">A-Spec</TextBlock>: Player can participate in the race and drive the car.
                                        <LineBreak/>
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">play_style->play_type</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>

                            <ComboBox Name="cb_PlayType" Height="23" Width="200" IsEnabled="False" TabIndex="13" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="91" Canvas.Top="101" SelectionChanged="cb_PlayType_SelectionChanged" />

                            <Label Content="Max Cars" Canvas.Left="10" Canvas.Top="130" Width="76">
                                <Label.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">Max Cars</TextBlock>
                                        <LineBreak/>
                                        How many cars are allowed on the track at once. <LineBreak/>
                                        Max is 16. <LineBreak/>
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold" Foreground="Red">IMPORTANT:</TextBlock> Be <TextBlock FontWeight="Bold">VERY</TextBlock> aware when using this value especially with (large) Custom Tracks. <LineBreak/>
                                        It affects directly the memory allocated for the entries and may cause a crash/freeze during track loading.
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->racers_max</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Name="iud_RacerMax" Canvas.Left="91" Canvas.Top="136" Width="200" Minimum="1" IsEnabled="False" Value="{Binding Path=RaceParameters.RacersMax}" Maximum="16" ValueChanged="iud_RacerMax_ValueChanged" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <Label Content="Event ID" Canvas.Left="10" Canvas.Top="161" Width="63">
                                <Label.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">Event ID</TextBlock>
                                        <LineBreak/>
                                        Unique Identifier for this Event. 
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">Note</TextBlock>: Only the first one can be edited - the program will automatically increment the other ones based on the first one.
                                        <LineBreak/>
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">event_id</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Name="iud_EventID" ValueChanged="iud_EventID_ValueChanged" IsEnabled="False" Canvas.Left="91" Canvas.Top="165" Width="200" Minimum="1" Value="{Binding EventID}" Maximum="9900000" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <GroupBox HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="318" Height="143" Width="281" >
                                <GroupBox.Header>
                                    <TextBlock Text="Event Stars"/>
                                </GroupBox.Header>
                                <GroupBox.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">Event Stars</TextBlock> <LineBreak/>
                                        How many stars are awarded in this event. They also work on non regular folders (i.e Missions). <LineBreak/>
                                        If you are using non regular folders it is recommended to set it to 1 star only, as the event will not show the star progression. <LineBreak/>
                                        <LineBreak/>
                                        <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">reward->star_table->star[]</TextBlock>
                                    </TextBlock>
                                </GroupBox.ToolTip>
                                <Canvas >
                                    <RadioButton Name="rdoStarsNone" IsEnabled="False" Height="24" Width="133" TabIndex="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Checked="rdoStarsNone_CheckedChanged" Canvas.Left="10" Canvas.Top="10" >No Stars</RadioButton>
                                    <RadioButton Name="rdoStarsOne" IsEnabled="False" Height="24" Width="133" TabIndex="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Checked="rdoStarsOne_CheckedChanged" Canvas.Left="10" Canvas.Top="39" >1 Star (Winning)</RadioButton>
                                    <RadioButton Name="rdoStarsThree" IsEnabled="False" Height="24" Width="133" TabIndex="1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Checked="rdoStarsThree_CheckedChanged" Canvas.Left="10" Canvas.Top="68" >3 Stars (1st/3rd/Any)</RadioButton>
                                </Canvas>
                            </GroupBox>
                            <Label Name="lblRaceName" Height="24" Content="Event Name" Width="76" TabIndex="2" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="11" >
                                <Label.ToolTip>
                                    <TextBlock>
                                        <TextBlock FontWeight="Bold" FontSize="14">Event Name</TextBlock>
                                        <LineBreak/>
                                        Name for your event (Can also be edited in the Event Information tab).
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <TextBox Name="txtEventName" Height="21" Width="200" TabIndex="3" IsEnabled="False" TextChanged="txtEventName_TextChanged" HorizontalAlignment="Right" VerticalAlignment="Bottom" TextWrapping="Wrap" Canvas.Left="91" Canvas.Top="14" />
                        </Canvas>
                    </GroupBox>
                    <GroupBox Header="Event Listing" Height="512" Canvas.Top="10" Canvas.Left="10" Width="310">
                        <Grid>
                            <Button Name="btnRemoveRace" Height="24" Width="96" TabIndex="4" IsEnabled="False" Click="btnRemoveRace_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="220" Canvas.Top="488" Margin="0,0,10,10" >Delete Selected</Button>
                            <Button Name="btnCopyRace" Height="24" Width="82" TabIndex="4" IsEnabled="False" Click="btnCopyRace_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="115" Canvas.Top="488" Margin="0,0,111,10" >Copy Selected</Button>

                            <Button Name="btnAddRace" Height="24" Width="90" TabIndex="1" Click="btnAddEvent_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="488" Margin="0,0,198,10" >Add Event</Button>
                            <ListBox Name="lstRaces" Height="422" Width="278" TabIndex="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" SelectionChanged="lstEvent_SelectedIndexChanged" Canvas.Left="10" Canvas.Top="32" Margin="0,0,10,39" />
                        </Grid>
                    </GroupBox>
                    <GroupBox HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="512" Canvas.Left="643" Width="295" Canvas.Top="10" >
                        <GroupBox.ToolTip>
                            <TextBlock>
                               <TextBlock FontWeight="Bold" FontSize="14">Image Card (GT6)</TextBlock>
                               <LineBreak/>
                                This is only available if your event is in a Mission, Coffee Break, One Make, Endurance, or License Folder.
                                <LineBreak/>
                                Recommended (and default) dimensions are <TextBlock FontStyle="Italic" >320x452</TextBlock> (Height:Width).
                            </TextBlock>
                        </GroupBox.ToolTip>
                        <GroupBox.Header>
                            <TextBlock Text="Image Card (GT6 Missions/One Make etc. only)"/>
                        </GroupBox.Header>
                        <Grid>
                            <Label Name="label_imagePreview" Height="26" TabIndex="15" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="640" Canvas.Top="423"
                                ToolTip="GT6 Only - Event Cover if the event is not in a regular folder (i.e mission/license)" Margin="0,0,66,65" Width="217">Special Event Image Preview</Label>
                            <Label Name="label_imagePreviewSize" Height="29" Width="55" TabIndex="15" HorizontalAlignment="Right" HorizontalContentAlignment="Right" VerticalAlignment="Top" VerticalContentAlignment="Top" Canvas.Left="883" Canvas.Top="420" Foreground="#FF646464" FontSize="11" Margin="0,371,6,0" >320x452</Label>

                            <Button Name="btnPickImage" Height="46" Width="130" TabIndex="18" Click="btnPickImage_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="649" Canvas.Top="458" Margin="0,0,143,10" >Pick Image</Button>
                            <Button Name="button7" Height="46" Width="119" TabIndex="17" Click="btnSaveImage_Click" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="808" Canvas.Top="458" Margin="0,0,10,10" >Save Image</Button>
                            <Border BorderThickness="1" BorderBrush="Gray" Canvas.Left="649" Canvas.Top="10" Margin="10,10,10,95">
                                <Image Name="pctImagePreview" Margin="0,2,0,2"/>
                            </Border>
                        </Grid>
                    </GroupBox>
                </Canvas>
            </TabItem>
            <TabItem Name="tabEventInfo" IsEnabled="False" >
                <TabItem.Header>
                    <StackPanel Orientation="Horizontal" >
                        <Image Width="16" Source="Resources/UI/TabLogos/EventInfo.png"/>
                        <TextBlock Text="Event Info" />
                    </StackPanel>
                </TabItem.Header>
                <Canvas Name="cantabEventInfo" >
                    <Label Content="Title/Event Name" Canvas.Left="10" Canvas.Top="10" >
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">Event Name</TextBlock>
                                <LineBreak/>
                                Name for your event. <LineBreak/>
                                <LineBreak/>
                                <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">information->title</TextBlock>
                             </TextBlock>
                        </Label.ToolTip>
                    </Label>
                    <TextBox Name="txt_EventTitle" Height="20" Width="469" TabIndex="1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="116" Canvas.Top="16" TextChanged="txtEventName_TextChanged"/>

                    <Label Content="One Line Title" Canvas.Top="41" Canvas.Left="10">
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">One Line Title</TextBlock> <LineBreak />
                                For GT6 only. <LineBreak />
                                One line title, will display in non regular folders, below the event card (scrolling if larger than screen width). <LineBreak/>
                                <LineBreak/>
                                <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">information->one_line_title</TextBlock>
                             </TextBlock>
                        </Label.ToolTip>
                    </Label>
                    <TextBox Name="txt_OneLineTitle" Height="20" Width="469" TabIndex="1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="116" Canvas.Top="44" TextChanged="txt_OneLineTitle_TextChanged" />

                    <Label Content="Description" Canvas.Top="72" Canvas.Left="10">
                        <Label.ToolTip>
                            <TextBlock>
                                <TextBlock FontWeight="Bold" FontSize="14">Description</TextBlock> <LineBreak />
                                For GT6 only. <LineBreak />
                                Description to show on the Event Info screen. Can have multiple lines and symbols. <LineBreak/>
                                <LineBreak/>
                                <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">information->description</TextBlock>
                             </TextBlock>
                        </Label.ToolTip>
                    </Label>
