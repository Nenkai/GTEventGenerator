                                    <TextBlock FontWeight="Bold" FontSize="14">End Date</TextBlock> <LineBreak />
                                    To be used if the event is a Seasonal Event.<LineBreak/>
                                    Must be within a valid time range, else you will not be able to enter the event. <LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">end_date</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:DateTimeUpDown Value="{Binding Path=EndDate}" TabIndex="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="78" Canvas.Top="45" Width="248" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

