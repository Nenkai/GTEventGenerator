                        <TextBlock Text="Event Settings" />
                    </StackPanel>
                </TabItem.Header>
                <Canvas >
                    <GroupBox HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="512" Width="256" Canvas.Left="682" Canvas.Top="10" >
                        <GroupBox.Header>
                            <TextBlock Text="Weather Parameters"/>
                        </GroupBox.Header>
                        <Canvas>
                            <Button Content="Setup Weather Steps" Canvas.Left="10" Canvas.Top="441" Width="224" Click="NewWeatherData_Click" Height="27"/>
                            <Label Name="label_WeatherAccelRetention" Content="Road Drying Speed" Height="27" Width="151" TabIndex="22" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="300">
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Road Drying Speed</TextBlock> <LineBreak />
                                    How fast the road can dry if wet. <LineBreak/>
                                    0 means it won't dry even if its sunny. 1000 means it will dry extremely quickly (Approx 7s).<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_accel_water_retention10</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.WeatherAccelWaterRetention}" Height="20" Width="68" Canvas.Left="166" Canvas.Top="303" Maximum="1000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <Label Name="label_TrackWetness" Content="Wetness at Race Start" Height="27" Width="151" TabIndex="22" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="328">
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Wetness at Race Start</TextBlock> <LineBreak />
                                    Adjusts how wet the track surface is at the start of the race. At 0%, the track is completely dry. <LineBreak/>
                                    As the value increases, the more water is present on the track, and the more slippery it will become. <LineBreak/>
                                    The road condition after the race starts will be dictated by factors such as the settings for "Wetness at Race Start" and "Road Wetting Speed." <LineBreak/>
                                    Initial Track Water in %, 1 is 10%, 100 is 100%. 0/-1 is no water on the track.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->initial_retention10</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.TrackWetness}" Height="20" Width="68" Canvas.Left="166" Canvas.Top="332" Maximum="10" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <ComboBox Name="comboBox_DecisiveWeather" Height="21" Width="102" TabIndex="37" HorizontalAlignment="Right" VerticalAlignment="Bottom" SelectionChanged="comboBox_DecisiveWeather_SelectedIndexChanged" Canvas.Left="132" Canvas.Top="361" />
                            <Label Name="label_DecisiveWeather" Content="Decisive Weather" Height="28" Width="117" TabIndex="24" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="357" >
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Decisive Weather</TextBlock> <LineBreak />
                                    Forced weather on the track which cannot change. If you want a variable weather, use None.<LineBreak/>
                                    <TextBlock FontWeight="Bold">None</TextBlock>: Use for variable weather.<LineBreak/>
                                    <TextBlock FontWeight="Bold">Sunny</TextBlock>: Recommended default. Sunny event.<LineBreak/>
                                    <TextBlock FontWeight="Bold">Rainy</TextBlock>: Rainy event.<LineBreak/>
                                    <TextBlock FontWeight="Bold">Snowy</TextBlock>: Snowy event.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->decisive_weather</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>

                            <Label Name="label_WeatherProgressionLength" Content="Weather Progress Length" Height="27" Width="153" TabIndex="22" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="386" >
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Weather Progression Length</TextBlock> <LineBreak />
                                    Total length in seconds of all weather steps.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_total_sec</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.WeatherTotalSec}" Height="20" Width="66" Canvas.Left="168" Canvas.Top="390" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <Label Name="label_WeatherRandomSeed" Content="Seed" Height="27" Width="37" TabIndex="20" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Top="180" Canvas.Left="132">
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Weather Random Seed</TextBlock> <LineBreak />
                                    Seed if the weather is random.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_random_seed</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.WeatherRandomSeed}" Height="20" Width="61" Canvas.Left="175" Canvas.Top="184" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <Label Name="label_WeatherTimeProgressionSpeed" Content="Clock Time Speed" Height="29" Width="140" TabIndex="18" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="240" >
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Clock Time Speed</TextBlock> <LineBreak />
                                    Choose how quickly time passes during a race. The greater this number is, the faster time passes during a race.<LineBreak/>
                                    Note: This setting is only available for some courses.<LineBreak/>
                                    <LineBreak/>
                                    Multiplier of how fast time progresses.<LineBreak/>
                                    - 1.0 = Real-Time, 60s/1min.<LineBreak/>
                                    - 24 = 2.5s/1min<LineBreak/>
                                    - 60 = 24 Minute day, 1s per minute.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->time_progress_speed</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.TimeProgressSpeed}" Height="20" Width="70" Canvas.Left="166" Canvas.Top="244" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <CheckBox Name="checkBox_RandomWeather" Content="Random Weather" Height="19" Width="119" TabIndex="17" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsChecked="{Binding Path=RaceParameters.WeatherRandom}" Canvas.Top="183" Canvas.Left="10">
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Random Weather</TextBlock> <LineBreak />
                                    Forces the weather to be random or predetermined.<LineBreak/>
                                    Can be used to make the game continue generating random weather after all preset weather steps are done.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_random</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>

                            <CheckBox Name="checkBox_WeatherPrecSnowOnly" Content="Only Snow" Height="24" Width="79" TabIndex="16" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsChecked="{Binding Path=RaceParameters.WeatherPrecSnowOnly}" Canvas.Top="211" Canvas.Left="157">
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Only Snow Precipitation</TextBlock> <LineBreak />
                                    Forces any allowed precipitation to be either rain or snow.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_prec_rain_only</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>

                            <CheckBox Name="checkBox_WeatherPrecRainOnly" Content="Only Rain Precipitation" Height="24" Width="141" TabIndex="15" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsChecked="{Binding Path=RaceParameters.WeatherPrecRainOnly}" Canvas.Top="211" Canvas.Left="10" >
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Only Rain Precipitation</TextBlock> <LineBreak />
                                    Forces any allowed precipitation to be either rain or snow.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_prec_snow_only</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>

                            <Label Name="label_WeatherPointNum" Content="Weather Step Count" Height="25" Width="153" TabIndex="13" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="414">
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Weather Step Count</TextBlock> <LineBreak />
                                    How many predetermined weather steps are setup. You shouldn't need to use that, as it will be set by the weather steps.<LineBreak/>
                                    Use "New Weather Data" to set them up, "Weather Progression Length" is the total length of all steps.<LineBreak/>
                                    Unimplemented for GT5.<LineBreak/>
                                    GT5 uses 4 max. GT6 is any amount.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_point_num</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Name="iud_WeatherPointNum" Value="{Binding Path=RaceParameters.WeatherPointNum}" IsEnabled="False" Height="20" Width="66" Canvas.Left="168" Canvas.Top="417" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <Label Name="label_WeatherChangeRate" Content="Road Wetting Speed" Height="26" Width="151" TabIndex="11" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="270">
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Weather Changeability</TextBlock> <LineBreak />
                                    Defines the speed at which weather changes during a race. <LineBreak/>
                                    A value of "0" will mean that the weather does not change at all, while higher values will result in increasingly changeable weather.<LineBreak/>
                                    <LineBreak/>
                                    Note: To make it clear, above is the original description, but somewhat misleading. <LineBreak />
                                    If this is set to 0, the track won't get wet even if its raining. 0 for no wet-ability, to 1000 for extremely fast.
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_accel10</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.WeatherAccel}" Height="20" Width="70" Canvas.Left="166" Canvas.Top="273" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <CheckBox Name="checkBox_WeatherNoWind" Content="No Wind" Height="19" Width="101" TabIndex="10" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsChecked="{Binding Path=RaceParameters.WeatherNoWind}" Canvas.Left="10" Canvas.Top="155">
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">No Wind</TextBlock> <LineBreak />
                                    Undocumented.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_no_wind</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>
                            <CheckBox Name="checkBox_WeatherNoSchedule" Content="No Schedule" Height="24" Width="101" TabIndex="9" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsChecked="{Binding Path=RaceParameters.WeatherNoSchedule}" Canvas.Left="10" Canvas.Top="126" >
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">No Schedule</TextBlock> <LineBreak />
                                    Whether or not the weather can vary.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_no_schedule</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>
                            <CheckBox Name="checkBox_WeatherNoPrecipitation" Content="No Precipitation" Height="24" Width="113" TabIndex="8" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsChecked="{Binding Path=RaceParameters.WeatherNoPrecipitation}" Canvas.Left="123" Canvas.Top="126" >
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">No Precipitation</TextBlock> <LineBreak />
                                    Whether or not the weather can progress to rain/snow.<LineBreak/>
                                    To enable 'Realtime Weather', disable this and enable weather_prec_rain/snow_only<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_no_precipitation</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>

                            <Label Name="label_MinTemp" Content="Min Temperature (in °C)" Height="26" Width="146" TabIndex="6" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="95" >
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Min Temperature</TextBlock> <LineBreak />
                                    Min temperature, affects how fast water can dry.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_min_celsius</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.WeatherMinCelsius}"  Height="20" Width="70" Canvas.Left="164" Canvas.Top="100" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <Label Name="label_MaxTemp" Content="Max Temperature (in °C)" Height="23" Width="145" TabIndex="4" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="67" >
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Max Temperature</TextBlock> <LineBreak />
                                    Max temperature, affects how fast water can dry.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_max_celsius</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.WeatherMaxCelsius}"  Height="20" Width="70" Canvas.Left="164" Canvas.Top="70" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <Label Name="label_BaseTemp" Content="Base Temperature (in °C)" Height="30" Width="144" TabIndex="3" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Top="37" Canvas.Left="10">
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Base Temperature</TextBlock> <LineBreak />
                                    Initial temperature, affects how fast water can dry.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->weather_base_celsius</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <xctk:IntegerUpDown Value="{Binding Path=RaceParameters.WeatherBaseCelsius}"  Height="20" Width="70" Canvas.Left="166" Canvas.Top="41" Maximum="100000" Minimum="-1" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>

                            <CheckBox Name="cb_StartTime" Height="24" Width="77" TabIndex="1" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="10" Canvas.Top="8" VerticalContentAlignment="Center" Unchecked="cb_StartTime_Checked" Checked="cb_StartTime_Checked"
                                      Content="Event Date">
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Event Date</TextBlock> <LineBreak />
                                    Choose the time of day at which the race takes place (dawn, daytime, night, etc.). This setting can only be changed for some courses. <LineBreak/>
                                    Uncheck for default. Date reflects seasons.<LineBreak/>
                                    Depending on where the track is located, January would have a very early sunset, or late sunrise.<LineBreak/>
                                    It is recommended to just set it to Summer, i.e June or better, use real dates if you are using real IRL events.<LineBreak/>
                                    Most tracks will be fully black if the date and time is incorrect.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->date</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                                <CheckBox.Resources>
                                    <Style TargetType="{x:Type Path}">
                                        <Setter Property="FlowDirection" Value="LeftToRight"/>
                                    </Style>
                                </CheckBox.Resources>
                            </CheckBox>
                            <xctk:DateTimeUpDown Name="date_Date" IsEnabled="False" Value="{Binding Path=RaceParameters.Date}"  Height="22" Width="142" TabIndex="0" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="92" Canvas.Top="10" Format="FullDateTime" BorderBrush="#323232" Background="#272727" Foreground="#FFFFFF"/>
                        </Canvas>
                    </GroupBox>
                    <GroupBox HorizontalAlignment="Right" VerticalAlignment="Bottom" Height="512" Width="667" Canvas.Top="10" Canvas.Left="10" >
                        <GroupBox.Header>
                            <TextBlock Text="General"/>
                        </GroupBox.Header>
                        <Canvas>
                            <CheckBox Name="checkBox_AutostartPitout" Content="Autostart Pitout" Height="24" Width="154" TabIndex="63" HorizontalAlignment="Right" VerticalAlignment="Bottom" IsChecked="{Binding RaceParameters.AutostartPitout}" Canvas.Left="501" Canvas.Top="126">
                                <CheckBox.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Autostart Pitout</TextBlock> <LineBreak />
                                    Unknown/Undocumented<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->autostart_pitout</TextBlock>
                                    </TextBlock>
                                </CheckBox.ToolTip>
                            </CheckBox>
                            <ComboBox Name="comboBox_Flagset" Height="21" Width="168" TabIndex="62" HorizontalAlignment="Right" VerticalAlignment="Bottom" SelectionChanged="comboBox_Flagset_SelectedIndexChanged" Canvas.Left="319" Canvas.Top="181" />
                            <Label Name="label_Flagset" Content="Flagset" Height="32" Width="54" TabIndex="61" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="260" Canvas.Top="177" >
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Flagset</TextBlock> <LineBreak />
                                    Unknown/Undocumented (GT5 only?)<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->flagset</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>

                            <Label Name="label_RaceType" Content="Race Type" Height="32" Width="70" TabIndex="61" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="260" Canvas.Top="207">
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Race Type</TextBlock> <LineBreak />
                                    Undocumented - would not touch<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->race_type</TextBlock>
                                    </TextBlock>
                                </Label.ToolTip>
                            </Label>
                            <ComboBox Name="comboBox_RaceType" Height="21" Width="157" TabIndex="62" HorizontalAlignment="Right" VerticalAlignment="Bottom" SelectionChanged="comboBox_RaceType_SelectedIndexChanged" Canvas.Left="330" Canvas.Top="212" />

                            <Label Name="label_MaxGhostLines" Content="Max Ghost Lines" Height="24" Width="131" TabIndex="59" HorizontalAlignment="Right" VerticalAlignment="Bottom" Canvas.Left="260" Canvas.Top="148" >
                                <Label.ToolTip>
                                    <TextBlock>
                                    <TextBlock FontWeight="Bold" FontSize="14">Max Ghost Lines</TextBlock> <LineBreak />
                                    The maximum ghost lines (GT6 Seasonals) to show, if enabled.<LineBreak/>
                                    <LineBreak/>
                                    <TextBlock FontWeight="Bold">XML</TextBlock>: <TextBlock FontStyle="Italic">race->line_ghost_play_max</TextBlock>
                                    </TextBlock>
