<Window x:Class="GTEventGenerator.CarParameter.CarParameterMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GTEventGenerator.CarParameter"
        mc:Ignorable="d"
        Title="CarParameterMainWindow" Height="296.57" Width="590.174">
    <Grid Background="#262626">
        <GroupBox Header="Main Settings" Margin="10,10,0,10" HorizontalAlignment="Left" Width="352">
            <Grid>
                <Label Content="Car:" Margin="10,10,230,165" />
                <Label Name="lbl_CarName" Content="- No Car Parameter Selected -" HorizontalContentAlignment="Right" Margin="49,10,10,165" />
            </Grid>
        </GroupBox>

        <GroupBox Header="Car Specific Settings" Margin="0,10,10,10" HorizontalAlignment="Right" Width="205">
