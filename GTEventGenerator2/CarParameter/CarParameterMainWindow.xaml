                <Button Content="Installed Car Parts" IsEnabled="False" Margin="10,10,10,0" Height="28" VerticalAlignment="Top"/>
                <Button Content="Car Settings" IsEnabled="False" Margin="10,43,10,0" Height="28" VerticalAlignment="Top"/>
                <Button Name="btn_PurchasedParts" IsEnabled="False" Content="Purchased Parts" Margin="10,76,10,0" Click="btn_PurchasedParts_Click" Height="28" VerticalAlignment="Top"/>

                <Button Name="btn_Import" Content="Import From Blob" Margin="10,0,10,10" Click="btn_Import_Click" Height="28" VerticalAlignment="Bottom"/>
            </Grid>
        </GroupBox>
    </Grid>
</Window>
